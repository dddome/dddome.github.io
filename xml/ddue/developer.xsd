    <!-- ************ PowerShell CmdLet Document ************ -->

  <complexType name="cmdletAliasesType">
    <sequence>
      <element name="cmdletAlias" type="string" maxOccurs="unbounded" />
    </sequence>
  </complexType>

  <complexType name="cmdletParametersType">
    <sequence minOccurs="0">
      <element name="cmdletParameter" minOccurs="0" maxOccurs="unbounded">
        <complexType>
          <sequence>
            <element name="parameterName" type="string" />
            <element name="type" type="string" />
            <element name="description" type="maml:cmdletDescriptionType"/>
            <element name="parameterSets">
              <complexType>
                <sequence>
                  <element name="parameterSet" maxOccurs="unbounded">
                    <complexType>
                      <simpleContent>
                        <extension base="string">
                          <attribute name="pipelineInput" />
                          <attribute name="valueFromRemArg" />
                          <attribute name="position" use="required" />
                          <attribute name="required" type="boolean" />
                          <attribute name="variableLength" type="boolean" />
                          <attribute name="helpMessage" />
                          <attribute name="helpMessageBaseName"/>
                          <attribute name="helpMessageResourceId"/>
                        </extension>
                      </simpleContent>
                    </complexType>
                  </element>
                </sequence>
                <attribute name="validateCount" />
                <attribute name="validateLength" />
                <attribute name="validateNotNull" type="boolean" />
                <attribute name="validateNotNullOrEmpty" type="boolean" />
                <attribute name="validatePattern" />
                <attribute name="validateRange" />
                <attribute name="validateScript" />
                <attribute name="validateSet" />
              </complexType>
            </element>
            <element name="parameterValueEnums" minOccurs="0">
              <complexType>
                <sequence>
                  <element name="parameterValueEnum" maxOccurs="unbounded">
                    <complexType>
                      <sequence>
                        <element name="name"/>
                        <element name="description" type="maml:cmdletDescriptionType"/>
                      </sequence>
                    </complexType>
                  </element>
                </sequence>
              </complexType>
            </element>
          </sequence>
          <attribute name="wildcard" type="boolean" />
          <attribute name="aliases" />
          <attribute name="defaultValue" />
          <attribute name="paramValueReq" type="boolean" use="required"/>
          <attribute name="paramValueVarLength" type="boolean" use="required"/>
        </complexType>
      </element>
    </sequence>
    <attribute name="supportsShouldProcess" type="boolean" default="false"/>
    <attribute name="supportsTransaction" type="boolean" default="false"/>
    <attribute name="commonParameters" type="boolean" default="true"/>
    <attribute name="defaultParameterSet" default="Default"/>
  </complexType>

  <complexType name="inputTypesType">
    <sequence>
      <element name="inputType" type="maml:inputOutputType" maxOccurs="unbounded" />
    </sequence>
  </complexType>

  <complexType name="outputTypesType">
    <sequence>
      <element name="outputType" type="maml:inputOutputType" maxOccurs="unbounded" />
    </sequence>
  </complexType>

  <complexType name="cmdletRemarksType">
    <sequence>
      <element name="cmdletRemark" maxOccurs="unbounded" type="maml:cmdletDescriptionType"/>
    </sequence>
  </complexType>

  <complexType name="cmdletCodeExamplesType">
    <choice maxOccurs="unbounded">
      <element name="cmdletCodeExample">
        <complexType>
          <sequence>
            <element name="title"/>
            <choice maxOccurs="unbounded">
              <element name="description" type="maml:cmdletDescriptionType"/>
              <element name="code">
                <complexType mixed="true">
                  <complexContent>
                    <extension base="maml:codeElementType">
                      <sequence minOccurs="0">
                        <element name="localizedText" type="maml:inlineType"/>
                      </sequence>
                      <attribute name="prompt" type="string" use="optional" default="C:\PS>"/>
                    </extension>
                  </complexContent>
                </complexType>
              </element>
              <element name="computerOutput" type="maml:inlineType" />
            </choice>
          </sequence>
        </complexType>
      </element>

      <element name="complexExample">
        <complexType>
          <sequence>
            <element name="title"/>
            <element name="description" type="maml:cmdletDescriptionType"/>
            <element name="cmdletCodeExample" maxOccurs="unbounded">
              <complexType>
                <choice maxOccurs="unbounded">
                  <element name="description" type="maml:cmdletDescriptionType"/>
                  <element name="code">
                    <complexType mixed="true">
                      <complexContent>
                        <extension base="maml:codeElementType">
                          <sequence minOccurs="0">
                            <element name="localizedText" type="maml:inlineType"/>
                          </sequence>
                          <attribute name="prompt" type="string" use="optional" default="C:\PS>"/>
                        </extension>
                      </complexContent>
                    </complexType>
                  </element>
                  <element name="computerOutput" type="maml:inlineType" />
                </choice>
              </complexType>
            </element>
          </sequence>
        </complexType>
      </element>
    </choice>
  </complexType>

  <complexType name="relatedLinksType">
    <sequence>
      <element name="onlineVersion" type="string" minOccurs="0" />
      <group ref="maml:relatedTopicLinkGroup" minOccurs="0" maxOccurs="unbounded"/>
    </sequence>
  </complexType>

  <complexType name="cmdletSynopsisDescriptionType">
    <sequence>
      <element name="synopsis" type="string" />
      <element name="detailedDescription" type="maml:cmdletDescriptionType"/>
    </sequence>
    <attribute name="verb" use="required"/>
    <attribute name="noun" use="required"/>
  </complexType>

